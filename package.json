{
  "name": "@typeql/core",
  "description": "Auto generated GraphQL schema from your TypeORM Models with dataloader",
  "version": "0.0.1-beta.1",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": "github:vinpac/typeql",
  "author": "Vinicius Pacheco <vin175pacheco@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "graphql",
    "typeorm",
    "dataloader"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.0",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/jest": "^24.0.18",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "add": "^2.0.6",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.3.0",
    "graphql": "^14.3.0",
    "graphql-tools": "^4.0.5",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.3.0",
    "merge-graphql-schemas": "^1.7.0",
    "mysql": "^2.17.1",
    "pg": "^7.11.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "prettier": "prettier --write \"{src,test,examples}/**/*.{js,ts,tsx}\"",
    "lint": "eslint {src,test}/**/* --no-ignore",
    "test": "jest --runInBand",
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "rm -rf dist && tsc"
  },
  "dependencies": {
    "dataloader": "^1.4.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "pluralize": "^8.0.0",
    "type-graph-orm": "^0.1.29"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
